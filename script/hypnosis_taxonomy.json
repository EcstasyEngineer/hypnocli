{
  "version": "5.2",
  "phases": {
    "P1": {
      "name": "Context Setting",
      "required": true,
      "duration_s": [30, 60],
      "words": [50, 100],
      "function": "Establish participation frame; set anchors; start authority/rapport; boundary normal to hypnotic.",
      "entry": "Session start",
      "exit": "Oriented, focused attention",
      "success": "Breathing settled, focus established",
      "use_when": "First-time listeners; any time context needs establishing",
      "skip_when": "Looping/series after episode 1; ultra-short runtime; pre-conditioned audience",
      "primary_techniques": ["INDU-03", "AUTH-01", "INDU-01"],
      "secondary_techniques": ["INDU-04", "DEEP-05", "IMMR-05"],
      "note": "Safety techniques (SAFE-*) are available but not mandatory. Use when audience/platform requires explicit safety framing."
    },
    "P2": {
      "name": "Induction",
      "required": true,
      "duration_s": [60, 90],
      "words": [100, 150],
      "function": "Transition consciousness; narrow attention; reduce movement; establish cooperation.",
      "entry": "Context set",
      "exit": "Initial trance, eyes closed",
      "success": "Relaxation response, absorption",
      "use_when": "Standard/first sessions; whenever no instant trigger is assumed",
      "skip_when": "Using instant/conditioned induction (INDU-06) in looping/series; time budget <4 min",
      "primary_techniques": ["INDU-02", "IMMR-01", "INDU-01", "INDU-03", "ABSR-02"],
      "secondary_techniques": ["DEEP-05", "INDU-06", "ABSR-01"],
      "note": "Re-induction (same-session) is just P2 again using INDU-06 or TRIG-02."
    },
    "P3": {
      "name": "Deepening",
      "required": true,
      "duration_s": [90, 180],
      "words": [150, 300],
      "function": "Increase depth; create progression; set depth markers; install first deepening triggers.",
      "entry": "Initial trance",
      "exit": "Deeper trance, reduced critical faculty",
      "success": "Countdown accepted; markers in place",
      "use_when": "Any script requiring depth before suggestions",
      "skip_when": "Looping variant with short runtime; ultra-short scripts",
      "primary_techniques": ["DEEP-01", "DEEP-02", "IMMR-01", "INDU-02", "DEEP-03"],
      "secondary_techniques": ["AUTH-02", "AUTH-01", "PLEA-01", "DEEP-04"],
      "note": "Fractionation (DEEP-03) is used here when depth amplification via wake/sleep cycling is needed."
    },
    "P4": {
      "name": "Core Suggestion + Immersion",
      "required": true,
      "duration_s": [120, 300],
      "words": [200, 500],
      "function": "Install primary suggestions; convert state to identity/behavior; reward linkage; persistence; experiential visualization; sensory layering.",
      "entry": "Depth achieved (or M1 Critical Softening completed)",
      "exit": "Core suggestions accepted, immersed in scene",
      "success": "Identity/behavior shifts acknowledged, sensory buy-in",
      "use_when": "Any goal beyond relaxation; triggers/behavior/identity changes; transformation/narrative themes",
      "skip_when": "None for functional hypnosis; only reduce density if runtime is under ~5 min",
      "primary_techniques": ["ENCD-01", "AUTH-02", "PLEA-01", "PERS-01", "ENCD-02", "IMMR-01", "IMMR-02", "IMMR-03", "IMMR-04", "IMMR-05", "IMMR-06", "IMMR-07", "IMMR-08"],
      "secondary_techniques": ["AUTH-07", "ENCD-04", "PERS-03"]
    },
    "P5": {
      "name": "Emergence",
      "required": true,
      "duration_s": [30, 90],
      "words": [50, 150],
      "function": "Safe return; maintain installs; re-energize; close frame.",
      "entry": "Suggestions installed",
      "exit": "Normal consciousness",
      "success": "Alert, oriented, positive affect",
      "use_when": "All non-looping scripts; anytime duty-of-care applies",
      "skip_when": "Looping variant with M4; live sessions transitioning to another induction immediately",
      "primary_techniques": ["EMRG-01", "EMRG-02", "SAFE-05", "EMRG-06"],
      "secondary_techniques": ["EMRG-03", "EMRG-04", "EMRG-07", "EMRG-08", "EMRG-09", "EMRG-10"],
      "critical": "Use EMRG category for emergence. Do NOT use DEEP-03 here."
    }
  },
  "modules": {
    "M1": {
      "name": "Critical Softening",
      "required": false,
      "insert_after": "P3",
      "duration_s": [60, 120],
      "words": [100, 200],
      "function": "Suspend analytical thinking; create blank state; voice replaces thoughts.",
      "entry": "After P3 (Deepening)",
      "exit": "Critical faculty suspended",
      "success": "Thought cessation accepted",
      "use_when": "Installing identity/behavioral change; authoritarian/challenge styles; complex triggers; deep programming",
      "skip_when": "Light relaxation/wellness; short scripts where P3 carries light thought-quieting",
      "primary_techniques": ["ABSR-01", "ABSR-02", "ABSR-08", "DISS-01"],
      "secondary_techniques": ["ABSR-03", "ABSR-04", "ABSR-05", "ABSR-06", "ABSR-07", "AUTH-04", "AUTH-05"]
    },
    "M2": {
      "name": "Transfer",
      "required": false,
      "insert_after": "P4",
      "duration_s": [60, 120],
      "words": [100, 200],
      "function": "Install conditioned responses; real-world commands; post-hypnotic actions; generalization to waking life.",
      "entry": "After P4 (Core Suggestion)",
      "exit": "Triggers encoded, actions accepted",
      "success": "Associations formed, bridge to real-world established",
      "use_when": "Series/looping; behavioral bridges; future sessions; habit installation",
      "skip_when": "Single-use relaxation; no post-hypnotic need; no external ask",
      "primary_techniques": ["TRIG-01", "TRIG-03", "TRIG-04", "ENCD-04", "PERS-04"],
      "secondary_techniques": ["AUTH-02", "PLEA-01", "PERS-06", "TRIG-09"]
    },
    "M3": {
      "name": "Demonstration",
      "required": false,
      "insert_after": "P4",
      "duration_s": [60, 180],
      "words": [100, 300],
      "function": "Activate installed triggers; sustain bliss states; provide subjective proof of hypnosis. The 'fun time' module.",
      "entry": "After P4 or M2",
      "exit": "Proof/pleasure experienced",
      "success": "Listener perceives phenomenon; desire to stay/return",
      "use_when": "Challenge style; reward loops; pleasure themes; low-belief listeners needing proof",
      "skip_when": "Risk of failure (asynchronous); wellness contexts where tests feel invasive",
      "primary_techniques": ["TRIG-02", "VALD-03", "VALD-06", "PLEA-01"],
      "secondary_techniques": ["INDU-06", "IMMR-01", "IMMR-08", "VALD-04", "PLEA-06"]
    },
    "M4": {
      "name": "Maintenance/Loop",
      "required": false,
      "insert_after": "P4",
      "terminal": true,
      "duration_s": [60, 90],
      "words": [100, 150],
      "function": "Sustain trance indefinitely; prepare loop restart; no emergence.",
      "entry": "After P4 or M2/M3 (loop terminal)",
      "exit": "Stable loop state",
      "success": "State mirrors opening; listener can continue",
      "use_when": "Looping/ambient content; background playback",
      "skip_when": "Any script requiring wake/aftercare",
      "primary_techniques": ["PERS-05", "PERS-03", "ABSR-02", "VALD-04"],
      "secondary_techniques": ["ENCD-05"],
      "note": "M4 replaces P5 (Emergence) as the terminal state."
    }
  },
